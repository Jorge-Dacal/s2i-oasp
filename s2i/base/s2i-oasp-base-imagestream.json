{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {
    "name": "s2i-oasp-base"
  },
  "items": [
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "s2i-oasp-base"
      },
      "spec": {
        "triggers": [
          {
            "type": "GitHub",
            "github": {
              "secret": "secret"
            }
          },
          {
            "type": "Generic",
            "generic": {
              "secret": "secret"
            }
          },
          {
            "type": "ImageChange",
            "imageChange": {}
          }
        ],
        "source": {
          "type": "Git",
          "git": {
            "uri": "https://github.com/mickuehl/s2i-oasp.git",
            "ref": "master"
          }
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "dockerfilePath": "s2i/base/Dockerfile"
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "namespace": "oasp",
            "name": "s2i-oasp-base:latest"
          }
        },
        "resources": {}
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "s2i-oasp-base"
      },
      "spec": {
        "tags": [
          {
            "name": "latest",
            "annotations": {
              "description": "The Open Application Standard Platform S2I base image",
              "tags": "builder,oasp",
              "supports": "oasp:2.4",
              "sampleRepo": "https://github.com/mickuehl/s2i-oasp.git",
              "version": "1.0"
            }
          }
        ]
      }
    }
  ]
}